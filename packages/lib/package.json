{
  "name": "@abstract-crypto/aztec-snap-lib",
  "version": "0.1.0",
  "private": true,
  "description": "A MetaMask snap libary for Aztec Snap",
  "repository": {
    "type": "git",
    "url": "https://github.com/porco-rosso-j/aztec-snap.git"
  },
  "license": "(MIT-0 OR Apache-2.0)",
  "type": "module",
  "main": "./dest/index.js",
  "types": "./dest/types.d.ts",
  "scripts": {
    "build": "tsc -b",
    "build:clean": "yarn clean && rm tsconfig.tsbuildinfo && rm -r dest && yarn build",
    "clean": "rimraf dist",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore"
  },
  "dependencies": {
    "@abstract-crypto/aztec-snap": "workspace:^",
    "@aztec/aztec.js": "0.24.0",
    "@aztec/noir-contracts.js": "0.24.0",
    "@aztec/types": "0.24.0",
    "@metamask/providers": "^15.0.0",
    "buffer": "^6.0.3"
  },
  "devDependencies": {
    "@lavamoat/allow-scripts": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.2.1",
    "prettier-plugin-packagejson": "^2.2.11",
    "rimraf": "^3.0.2",
    "typescript": "5.0.4"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
