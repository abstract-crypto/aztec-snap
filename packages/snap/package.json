{
  "name": "@ziad-saab/dogecoin-snap",
  "version": "0.1.0",
  "private": true,
  "description": "A MetaMask snap for managing Dogecoin assets",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/template-snap-monorepo.git"
  },
  "license": "(MIT-0 OR Apache-2.0)",
  "main": "src/index.ts",
  "files": [
    "dist/",
    "images/",
    "snap.manifest.json"
  ],
