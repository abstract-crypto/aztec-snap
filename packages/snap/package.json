{
  "name": "@abstract-crypto/aztec-snap",
  "version": "0.1.0",
  "private": true,
  "description": "A MetaMask snap for Aztec Sandbox",
  "repository": {
    "type": "git",
    "url": "https://github.com/porco-rosso-j/aztec-snap.git"
  },
  "license": "(MIT-0 OR Apache-2.0)",
  "type": "module",
  "types": "./dest/index.d.ts",
  "files": [
    "dest/",
    "dist/",
    "images/",
    "snap.manifest.json"
  ],
  "scripts": {
    "clean": "rm dist/bundle.js",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "serve": "mm-snap serve",
    "start": "mm-snap watch",
    "start:clean": "yarn clean && mm-snap watch",
    "build": "mm-snap build",
    "build:types": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "@aztec/accounts": "0.41.0",
    "@aztec/aztec.js": "0.41.0",
    "@aztec/circuits.js": "0.41.0",
    "@aztec/noir-contracts.js": "0.41.0",
    "@metamask/snaps-sdk": "3.0.1",
    "buffer": "^6.0.3",
    "webpack": "^5.90.3"
  },
  "devDependencies": {
    "@lavamoat/allow-scripts": "^2.0.3",
    "@metamask/key-tree": "9.0.0",
    "@metamask/snaps-cli": "6.0.2",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "esbuild-loader": "^4.1.0",
    "eslint": "^8.13.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.6.2",
    "resolve-typescript-plugin": "^2.0.1",
    "rimraf": "^3.0.2",
    "stream-browserify": "^3.0.0",
    "string_decoder": "^1.3.0",
    "ts-loader": "9.4.4",
    "tty-browserify": "^0.0.1",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
